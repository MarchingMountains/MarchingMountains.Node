{
  "name": "MarchingMountains",
  "description": "Marching Mountains web site ",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/IanFelton/MarchingMountains.Node.git"
  },
  "main": "server.js",
  "engines": {
    "node": "5.9.0",
    "npm": "3.7.3"
  },
  "scripts": {
    "start": "babel-node server.js",
    "watch": "nodemon --exec babel-node -- server.js",
    "postinstall": "bower install && gulp build",
    "test": "jest --coverage",
    "uat": "nightwatch",
    "uat_prod": "nightwatch -e prod"
  },
  "jest": {
    "testDirectoryName": "specs",
    "testFileExtensions": [
      "spec.js"
    ],
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/bluebird",
      "<rootDir>/node_modules/request-promise",
      "<rootDir>/node_modules/alt",
      "<rootDir>/app/"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  },
  "dependencies": {
    "alt": "^0.17.1",
    "angular": "1.5.2",
    "async": "^1.4.0",
    "babel": "^6.1.18",
    "babel-cli": "^6.1.18",
    "babel-core": "^6.1.21",
    "babel-jest": "^6.0.1",
    "babel-loader": "6.1.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babelify": "^7.2.0",
    "body-parser": "1.15.0",
    "bower": "^1.4.1",
    "browserify": "^11.0.0",
    "colors": "^1.1.2",
    "compression": "^1.5.1",
    "cookie-parser": "^1.3.3",
    "express": "4.13.4",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-concat": "^2.6.0",
    "gulp-cssmin": "^0.1.7",
    "gulp-if": "^1.2.5",
    "gulp-less": "^3.0.3",
    "gulp-plumber": "^1.0.1",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.6",
    "history": "1.13.1",
    "jest": "0.1.40",
    "jest-cli": "0.7.1",
    "method-override": "^2.2.0",
    "mocha": "2.3.4",
    "morgan": "1.7.0",
    "nightwatch": "0.8.6",
    "react": "0.14.2",
    "react-addons-test-utils": "0.14.2",
    "react-dom": "0.14.2",
    "react-router": "0.13.x",
    "request": "^2.58.0",
    "serve-favicon": "^2.3.0",
    "swig": "^1.4.2",
    "underscore": "^1.8.3",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.3.0",
    "webpack": "^1.12.6"
  },
  "devDependencies": {},
  "license": "UNLICENSED",
  "private": "true"
}
